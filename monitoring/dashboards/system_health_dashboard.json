{
  "dashboard": {
    "id": null,
    "title": "Medical AI System Health Monitoring",
    "tags": ["medical-ai", "health", "system"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"medical-ai-backend\"}",
            "refId": "A",
            "legendFormat": "Backend Services"
          },
          {
            "expr": "up{job=\"medical-ai-model-serving\"}",
            "refId": "B",
            "legendFormat": "Model Serving"
          },
          {
            "expr": "up{job=\"medical-ai-database\"}",
            "refId": "C",
            "legendFormat": "Database"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "CPU and Memory Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "refId": "A",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "refId": "B",
            "legendFormat": "Memory Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 3,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(medical_ai_database_query_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "95th Percentile Query Latency"
          },
          {
            "expr": "rate(medical_ai_database_errors_total[5m])",
            "refId": "B",
            "legendFormat": "Error Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 4,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(medical_ai_requests_total[5m])",
            "refId": "A",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Error Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(medical_ai_requests_total{status!~\"2..\"}[5m])) by (service) / sum(rate(medical_ai_requests_total[5m])) by (service) * 100",
            "refId": "A",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Disk Space Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "(node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100",
            "refId": "A",
            "legendFormat": "Available Space %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 20},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total[5m])",
            "refId": "A",
            "legendFormat": "Network In"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total[5m])",
            "refId": "B",
            "legendFormat": "Network Out"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 8,
        "title": "Kubernetes Pods Health",
        "type": "table",
        "targets": [
          {
            "expr": "kube_pod_status_phase",
            "refId": "A",
            "legendFormat": "Pod Status"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      }
    ]
  }
}