# Environment Configuration Example
# Copy this file to .env and configure for your environment

# Application Environment
APP_ENVIRONMENT=development
APP_DEBUG=true

# Model Configuration
MODEL_MODEL_NAME=microsoft/DialoGPT-medium
MODEL_MODEL_PATH=
MODEL_MODEL_REVISION=main
MODEL_MAX_LENGTH=512
MODEL_TEMPERATURE=0.7
MODEL_TOP_P=0.9
MODEL_TOP_K=50
MODEL_USE_QUANTIZATION=true
MODEL_QUANTIZATION_TYPE=4bit
MODEL_LOAD_IN_8BIT=false
MODEL_LOAD_IN_4BIT=true
MODEL_TORCH_DTYPE=float16

# Server Configuration
SERVING_HOST=0.0.0.0
SERVING_PORT=8000
SERVING_WORKERS=4
SERVING_API_KEY=
SERVING_ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:8080"]
SERVING_CORS_ALLOW_CREDENTIALS=true
SERVING_CORS_ALLOW_METHODS=["GET","POST","PUT","DELETE"]
SERVING_CORS_ALLOW_HEADERS=["*"]
SERVING_RATE_LIMIT_PER_MINUTE=60
SERVING_RATE_LIMIT_BURST=10
SERVING_MAX_REQUEST_SIZE=10485760
SERVING_MAX_CONCURRENT_REQUESTS=10
SERVING_REQUEST_TIMEOUT=300

# Cache Configuration
CACHE_REDIS_URL=redis://localhost:6379/0
CACHE_REDIS_PASSWORD=
CACHE_REDIS_SSL=false
CACHE_CACHE_TTL=3600
CACHE_MAX_CACHE_SIZE=1000
CACHE_ENABLE_MODEL_CACHE=true
CACHE_ENABLE_RESPONSE_CACHE=true
CACHE_DISK_CACHE_DIR=./cache
CACHE_DISK_CACHE_SIZE=10737418240

# Logging Configuration
LOG_LOG_LEVEL=INFO
LOG_LOG_FORMAT=json
LOG_LOG_FILE=./logs/serving.log
LOG_LOG_FILE_MAX_SIZE=104857600
LOG_LOG_FILE_BACKUP_COUNT=5
LOG_ENABLE_FILE_LOGGING=true
LOG_ENABLE_CONSOLE_LOGGING=true
LOG_CONSOLE_LOG_FORMAT="%(asctime)s - %(name)s - %(levelname)s - %(message)s"
LOG_ENABLE_STRUCTURED_LOGGING=true
LOG_INCLUDE_REQUEST_ID=true
LOG_INCLUDE_USER_ID=true
LOG_ENABLE_METRICS=true
LOG_METRICS_PORT=9090
LOG_SENTRY_DSN=

# Medical Compliance Configuration
MEDICAL_ENABLE_ENCRYPTION=true
MEDICAL_ENCRYPTION_KEY=
MEDICAL_PHI_REDACTION=true
MEDICAL_ALLOWED_PHI_FIELDS=["patient_id","timestamp"]
MEDICAL_PHI_LOGGING=false
MEDICAL_DATA_RETENTION_DAYS=30
MEDICAL_AUTO_DELETE=true
MEDICAL_ENABLE_AUDIT_LOG=true
MEDICAL_AUDIT_LOG_FILE=./logs/audit.log
MEDICAL_ENABLE_RBAC=true
MEDICAL_DEFAULT_ROLE=user

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/medical_ai
DATABASE_ECHO=false
DATABASE_POOL_SIZE=5
DATABASE_MAX_OVERFLOW=10
DATABASE_ENABLE_ALEMBIC=true
DATABASE_ALEMBIC_PATH=alembic