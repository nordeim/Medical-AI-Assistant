{
  "platform_name": "Advanced Analytics Platform",
  "version": "1.0.0",
  "environment": "development",
  "debug_mode": true,
  "database_url": "postgresql://user:password@localhost:5432/analytics",
  "cache_enabled": false,
  "cache_ttl": 3600,
  "ml_confidence_threshold": 0.75,
  "auto_model_training": true,
  "model_retrain_interval": 24,
  "prediction_batch_size": 1000,
  "insight_confidence_threshold": 0.7,
  "insight_impact_threshold": 0.6,
  "data_quality_threshold": 0.8,
  "max_concurrent_analyses": 5,
  "timeout_seconds": 300,
  "memory_limit_mb": 2048,
  "encryption_enabled": false,
  "audit_logging": false,
  "data_retention_days": 365,
  "api_rate_limit": 1000,
  "external_apis_enabled": false,
  "webhook_callbacks": true,
  "email_notifications": true,
  "dashboard_alerts": true,
  "threshold_violation_alerts": true
}